/**
 * Implements hook_menu().
 */
function token_menu() {
  $items['token/autocomplete'] = array(
    'page callback' => 'token_autocomplete',
    'access callback' => TRUE,
  );
  return $items;
}

function token_autocomplete() {
  $args = func_get_args();
  $string = implode('/', $args);
  
  echo $string;
}
